<?xml version="1.0" encoding="UTF-8"?>
<local:HelloWorldBase
        xmlns:exml="http://www.jangaroo.net/exml/0.8"
        xmlns:fx="http://ns.adobe.com/mxml/2009"
        xmlns="ext:widget"
        xmlns:local="com.acme.extas.localized.*">
  <fx:Script><![CDATA[
    public native function HelloWorld(config:HelloWorld = null);
    ]]></fx:Script>
  <fx:Declarations>
    <local:HelloWorld id="config"/>
  </fx:Declarations>
  <local:items>
    <Panel title="{BUNDLE.panel_title}">
      <tbar>
        <Toolbar>
          <items>
            <Button text="{BUNDLE.button_label}" handler="{onClick}"/>
            <Button text="{BUNDLE.menu_label}">
              <menu>
                <local:LocaleMenu/>
              </menu>
            </Button>
          </items>
        </Toolbar>
      </tbar>
    </Panel>
  </local:items>
</local:HelloWorldBase>
